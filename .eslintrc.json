{
  "env": {
      "browser": true,
      "es6": true
  },
  "extends": "airbnb-base",
  "parserOptions": {
      "ecmaVersion": 2018,
      "sourceType": "module"
  },
  "rules": {
      "linebreak-style":"off",
      "no-underscore-dangle":"off",
      "no-plusplus":"off",
      "no-console":"off",
      "template-curly-spacing":"off",
      "import/extensions":"off"
  }
}
